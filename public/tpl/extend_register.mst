<div class="col padding-small padding-8 transition-slow extend_register radius" rel="{{id}}" style="height:46px;overflow: hidden">
    <div class="col s-padding-bottom">
        <div class="col right" style="overflow:hidden; height:30px; width: {{^columnStatus.have}}46{{/columnStatus.have}}{{#columnStatus.have}}100{{/columnStatus.have}}px">
            <button onclick="deleteExtendMult('{{columnName}}', '{{id}}')" rel="{{id}}" class="right btn-small radius color-gray-light hover-theme opacity hover-opacity-off btn-del-extend-register transition-slow"
                    data-column="{{columnName}}" data-id="{{id}}" style="overflow: hidden;margin-left: 5px; width: 38px" >
                <i class="material-icons left font-large">delete</i>
            </button>
            {{#columnStatus.have}}
                <label class="right" style="height: 35px;">
                    <div class="switch switch-squad" style="margin:-8px 3px 0 0">
                        <input type="checkbox" class="switch-status-extend switchCheck"
                               data-status="{{columnStatus.value}}"
                               data-column="{{columnName}}"
                               data-entity="{{entity}}"
                               data-id="{{id}}"
                               rel="{{formIdentificador}}"
                               {{#columnStatus.value}}checked='checked'{{/columnStatus.value}}
                        />
                        <div class="slider"></div>
                    </div>
                </label>
            {{/columnStatus.have}}
        </div>
        <div class="rest pointer no-select" onclick="edtiExtendMult('{{columnRelation}}', '{{columnName}}', '{{id}}')">
            <button class="left btn-small radius theme hover-theme opacity hover-opacity-off btn-edit-extend-register transition-slow" rel="{{id}}">
                <i class="material-icons left font-large transition-slow">edit</i>
            </button>
            <div style="height: 30px" class="rest font-medium padding-4 padding-left extend_register_title" rel="{{id}}">{{{columnTituloExtend}}}</div>
        </div>
    </div>
    <div class="col extend_register_edit transition-slow" style="height: 0;overflow: hidden">
    </div>
</div>